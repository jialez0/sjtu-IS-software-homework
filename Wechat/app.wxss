/**app.wxss**/
@import "colorui/main.wxss";
@import "colorui/icon.wxss";

.countdown {
  width:100%;
  height:10vh;
  text-align: center;
  font-size: 44rpx;
}
.my-map {
  width:100%;
  height:80vh;
}
.passport {
  width:100%;
  height:20vh;
}
.numinput {
  border: 1rpx solid#e8e8e8;
  width: 220rpx;
  margin: 30rpx auto;
  padding: 30rpx;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
